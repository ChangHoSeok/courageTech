	<entry key="messagePush.page.enable">1</entry>
	<entry key="messagePush.page.size">10</entry>
	<entry key="messagePush.page.recordCount">30</entry>
	
	<!-- 게시판 Master -->
	<entry key="boardMaster.page.enable">1</entry>
	<entry key="boardMaster.page.size">10</entry>
	<entry key="boardMaster.page.recordCount">30</entry>
	
	<!-- 게시판 -->
	<entry key="board.page.enable">1</entry>
	<entry key="board.page.size">10</entry>
	<entry key="board.page.recordCount">30</entry>
	
	<!-- 댓글 -->
	<entry key="answer.page.enable">0</entry>
	<entry key="answer.page.size">10</entry>
	<entry key="answer.page.recordCount">50</entry>
	
	<!-- 게시판 권한관리 -->
	<entry key="boardAuthor.page.enable">0</entry>
	<entry key="boardAuthor.page.size">10</entry>
	<entry key="boardAuthor.page.recordCount">30</entry>
	
